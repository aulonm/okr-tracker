{
  "name": "okr-tracker",
  "version": "2.7.5",
  "private": true,
  "scripts": {
    "test:unit": "NODE_ENV=test jest",
    "dev": "firebase emulators:start --import=./mock_data --export-on-exit=mock_data & vite",
    "dev:test": "firebase emulators:start --import=./tests & vite",
    "build": "vite build --mode production",
    "build:test": "vite build --mode staging",
    "test:e2e": "firebase emulators:start --import=./tests & vite & env-cmd -f ./.env.local cypress open",
    "test:e2e:headless": "firebase emulators:exec --project origo-okr-tracker --import=./tests & vite & (sleep 10 && env-cmd -f ./.env.local cypress run --headless)",
    "deploy": "npm run build && firebase deploy --project=production",
    "deploy:test": "npm run build:test && firebase deploy --project=development",
    "deploy:feature_branch": "npm run build && firebase hosting:channel:deploy --project=production $npm_config_name",
    "lint": "eslint ./src",
    "lint:style": "stylelint 'src/**/*.scss' 'src/**/*.vue'",
    "lint:style:fix": "stylelint 'src/**/*.scss' 'src/**/*.vue' --fix",
    "export_mock_data": "firebase emulators:export ./mock_data"
  },
  "dependencies": {
    "@braid/griddle": "^2.3.1",
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@vitejs/plugin-vue2": "^1.1.2",
    "d3-array": "^3.2.0",
    "d3-axis": "^3.0.0",
    "d3-ease": "^3.0.1",
    "d3-format": "^3.1.0",
    "d3-hierarchy": "^3.1.2",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.1.0",
    "d3-transition": "^3.0.1",
    "date-fns": "^2.29.2",
    "dompurify": "^2.4.0",
    "firebase": "^8.10.0",
    "flatpickr": "^4.6.13",
    "fuse.js": "^6.6.2",
    "marked": "^4.1.0",
    "swagger-ui": "^4.14.0",
    "v-tooltip": "^2.1.3",
    "vee-validate": "^3.4.14",
    "vue": "^2.7.10",
    "vue-click-outside": "^1.1.0",
    "vue-content-loader": "^0.2.3",
    "vue-flatpickr-component": "^8.1.7",
    "vue-i18n": "^8.27.2",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.6.3",
    "vue-select": "^3.20.0",
    "vue-toasted": "^1.1.28",
    "vuefire": "^2.2.5",
    "vuex": "^3.6.2",
    "vuexfire": "^3.2.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.18.9",
    "@babel/preset-env": "^7.19.0",
    "@firebase/rules-unit-testing": "^1.3.15",
    "env-cmd": "^10.1.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "firestore-jest-mock": "^0.17.0",
    "firebase-tools": "^11.8.0",
    "jest": "^27.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.8",
    "stylelint": "^13.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0",
    "vite": "^3.1.0",
    "vue-template-compiler": "^2.7.10"
  },
  "resolutions": {
    "postcss": "8.3.11"
  }
}
